    }
  },
  {
    "Id": "essential_run_disk_cleanup",
    "Name": "Run Disk Cleanup",
    "Description": "Runs Disk Cleanup on Drive C: and removes old Windows Updates.",
    "Category": 1,
    "Type": 5,
    "Panel": 1,
    "Status": 0,
    "IsEnabled": false,
    "RequiresRestart": false,
